{
  "android": {
    "activityName": "MainActivity",
    "deviceName": "Nexus_5X",
    "packageName": "com.demo",
    "snapshotsPath": "./snapshots/android",
    "dev": {
      "deviceParams": [
        "-no-audio",
        "-no-snapshot"
      ]
    },
    "debug": {
      "deviceParams": [
        "-no-audio",
        "-no-snapshot"
      ],
      "appFile": "./android/app/build/outputs/apk/debug/app-debug.apk"
    },
    "test": {
      "port": 3000,
      "canStopDevice": false,
      "deviceParams": [
        "-no-audio",
        "-no-snapshot"
      ],
      "appFile": "./android/app/build/outputs/apk/debug/app-debug.apk"
    },
    "release": {
      "deviceParams": [
        "-no-audio",
        "-no-snapshot",
        "-no-window"
      ],
      "appFile": "./android/app/build/outputs/apk/release/app-release.apk"
    },
    "deviceStartTimeout": 80000
